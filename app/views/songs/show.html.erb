<div class="row">
  <h1>Songs for <%= @song.artist_name %></h1>
    <%= link_to "Edit artist", edit_artist_path(@artist) %>
  <div>
      <%= image_tag @artist.image %>
    </div>
    <ul>

      <div>
        <h2>All from this artist</h2>
      </div>

      <% @songs.each do |song| %>
      <li>
        <%= song.song_name %>
      </li>
      <% end %>
  
        </ul>
        <%= link_to "All Artists", root_path %>
      </div>
